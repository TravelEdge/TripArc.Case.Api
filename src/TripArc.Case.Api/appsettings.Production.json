{
  "Serilog": {
    "Using": [ "Serilog", "Serilog.Exceptions", "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Information",
        "Microsoft": "Information",
        "Microsoft.EntityFrameworkCore": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console",
              "Args": {
                "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Literate, Serilog.Sinks.Console",
                "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3} {Environment}] {Message:lj} <s:{SourceContext}>{NewLine}{Exception}"
              }
            }
          ]
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithExceptionDetails" ],
    "Properties": {
      "Environment": "Production"
    }
  },
  "ApplicationInsights": {
    "ConnectionString": "InstrumentationKey=47b236b4-0d5b-4515-9c22-8c87a361d46b;IngestionEndpoint=https://eastus-8.in.applicationinsights.azure.com/"
  },
  "ConnectionStrings": {
    "TMX": ""
  },
  "AuthenticationSettings": {
    "Authority": "",
    "ApiOrScopeName": "case_api"
  },
  "GeneralSettings": {
    "Version": "0.1",
    "Environment": "production"
  }
}